@model IEnumerable<Jobs.Models.Career>

<ul class="list-group">
    <li class="list-group-item active" style="font-weight:bold; text-align:center">
        DANH SÁCH NGHỀ NGHIỆP
    </li>
    @foreach (var item in Model)
    {
        <a href="@Url.Action("Career","Jobs", new { id = item.ID})"
           class="list-group-item">@item.Name</a>
    }
</ul>
